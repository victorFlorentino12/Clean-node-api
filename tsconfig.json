{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["src/*.spec.ts", "jest.config.ts", "src/*.ts", "src/**/**/*.spec.ts", "src/**/**/*.ts"], // Adicione seu arquivo aqui
  "exclude": ["node_modules"]
}
